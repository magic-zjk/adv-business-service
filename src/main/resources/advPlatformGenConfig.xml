<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<properties resource="generatorJdbc_base.properties" />
	<classPathEntry
		location="F:\mavenrepository\repository\mysql\mysql-connector-java\5.1.29\mysql-connector-java-5.1.29.jar" />
	location="E:\maven\repo\mysql\mysql-connector-java\5.1.29\mysql-connector-java-5.1.29.jar"
	/>
	<context id="DB2Tables" targetRuntime="MyBatis3">

		<!-- 对sql关键字设定分隔符 -->
		<property name="autoDelimitKeywords" value="true" />
		<property name="beginningDelimiter" value="`" />
		<property name="endingDelimiter" value="`" />
		<property name="javaFileEncoding" value="UTF-8" />

		<plugin
			type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin"></plugin>
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>
		<!-- 分页 -->
		<plugin
			type="com.imopan.adv.platform.mbg.plugin.PaginationPlugin" />
		<!-- toString() -->
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin" />

		<commentGenerator>
			<property name="suppressDate" value="true" />
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="false" />
		</commentGenerator>
		<!--数据库链接URL，用户名、密码 -->
		<jdbcConnection driverClass="${driverClass}"
			connectionURL="${connectionURL}" userId="${userId}"
			password="${password}">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<javaModelGenerator
			targetPackage="${javaModelTargetPackage}"
			targetProject="${targetProject_java}">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
			<property name="rootClass"
				value="${javaModelGenerator_rootClass}" />
		</javaModelGenerator>
		<sqlMapGenerator
			targetPackage="${sqlMapTargetPackage}"
			targetProject="${targetProject_resources}">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<javaClientGenerator
			targetPackage="${javaClientTargetPackage}"
			targetProject="${targetProject_java}" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table tableName="adv_info_cpa" domainObjectName="AdvInfoCpa"
			enableCountByExample="true" enableUpdateByExample="true"
			enableDeleteByExample="false" enableSelectByExample="true"
			selectByExampleQueryId="false">
		</table>


	</context>
</generatorConfiguration>